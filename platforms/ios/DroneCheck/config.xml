<?xml version='1.0' encoding='utf-8'?>
<widget id="com.derekhonerlaw.dronecheck" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="assets/iphone/appicon-60@3x.png" width="180" />
    <icon height="60" src="assets/iphone/appicon-60.png" width="60" />
    <icon height="120" src="assets/iphone/appicon-60@2x.png" width="120" />
    <icon height="76" src="assets/iphone/appicon-76.png" width="76" />
    <icon height="152" src="assets/iphone/appicon-76@2x.png" width="152" />
    <icon height="40" src="assets/iphone/appicon-Small-40.png" width="40" />
    <icon height="80" src="assets/iphone/appicon-Small-40@2x.png" width="80" />
    <icon height="57" src="assets/iphone/appicon.png" width="57" />
    <icon height="114" src="assets/iphone/appicon@2x.png" width="114" />
    <icon height="72" src="assets/iphone/appicon-72.png" width="72" />
    <icon height="144" src="assets/iphone/appicon-72@2x.png" width="144" />
    <icon height="29" src="assets/iphone/appicon-Small.png" width="29" />
    <icon height="58" src="assets/iphone/appicon-Small@2x.png" width="58" />
    <icon height="87" src="assets/iphone/appicon-Small@3x.png" width="87" />
    <icon height="50" src="assets/iphone/appicon-Small-50.png" width="50" />
    <icon height="100" src="assets/iphone/appicon-Small-50@2x.png" width="100" />
    <splash height="480" src="assets/iphone/Default.png" width="320" />
    <splash height="960" src="assets/iphone/Default@2x.png" width="640" />
    <splash height="1024" src="assets/iphone/Default-Portrait.png" width="768" />
    <splash height="2048" src="assets/iphone/Default-Portrait@2x.png" width="1536" />
    <splash height="1136" src="assets/iphone/Default-568h@2x.png" width="640" />
    <splash height="1334" src="assets/iphone/Default-667h@2x.png" width="750" />
    <splash height="2208" src="assets/iphone/Default-Portrait-736h@3x.png" width="1242" />
    <name>DroneCheck</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
